.container
    .row
        .col
            ul.nav.justify-content-center
                each item in navMenuItems
                    li.nav-item(
                        class=request.path.startsWith(item.url) ? "active" : ""
                    )
                        a.nav-link(
                            href=item.url
                        )=item.name
